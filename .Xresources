! XAUTOLOCK
Xautolock.timer: 5
Xautolock.locker: killall -SIGUSR1 dunst; i3lock --nofork --dpms -c 000000; killall -SIGUSR2 dunst

! FONT SETTINGS
!Xft.dpi:        125
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintslight
 
! TERMINAL
xterm*VT100.geometry:     80x25
xterm*faceName:           Dejavu Sans Mono:style=Regular:size=10
!xterm*font:              -*-dina-medium-r-*-*-16-*-*-*-*-*-*-*
xterm*dynamicColors:      true
xterm*utf8:               2
xterm*eightBitInput:      true
xterm*saveLines:          512
xterm*scrollBar:          false
xterm*toolBar:            false


*background:              #000000
!*foreground:              #babdb6
*foreground:              #dddddd
*color0:                  #2e3436
*color1:                  #cc0000
*color2:                  #4e9a06
*color3:                  #c4a000
*color4:                  #3465a4
*color5:                  #75507b
*color6:                  #06989a
*color7:                  #d3d7cf
*color8:                  #555753
*color9:                  #ef2929
*color10:                 #8ae234
*color11:                 #fce94f
*color12:                 #729fcf
*color13:                 #ad7fa8
*color14:                 #34e2e2
*color15:                 #eeeeec
